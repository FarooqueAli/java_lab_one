//7. Write a program where every single escape
//character is used and show the results.

class task7f
{
	public static void main(String args[])
	{
		String a="Java";
		String b="Programmer";
 		System.out.println("NEW LINE ESCAPE CHARACTER");
		System.out.println("Java\n"+"Programmer");
		System.out.println("BACK ESCAPE CHARACTER");
		System.out.println("Java \b"+"Programmer");
		System.out.println("TAB ESCAPE CHARACTER");
		System.out.println("Java\t"+"Programmer");
		System.out.println("SINGLE QOUTE ESCAPE CHARACTER");
		System.out.println("Java\'"+"Programmer");
		System.out.println("DOUBLE QOUTE ESCAPE CHARACTER");
		System.out.println("Java\""+"Programmer");
		System.out.println("CARAIGE ESCAPE CHARACTER");
		System.out.println("Java\r"+"Programmer");
		System.out.println("BACK SLASH ESCAPE");
		System.out.println("Java \\"+"Programmer");

	}

}
